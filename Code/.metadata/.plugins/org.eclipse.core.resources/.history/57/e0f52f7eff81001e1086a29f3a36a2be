package assignment_CAMsApp;

public class Camp {
	private Student[] students;
	private CampInformation campInfo;
	private Staff staffInCharge;
	private CampCommitteeMember[] committeeMembers;
	
	
	public Student[] getStudents() {
		return students;
	}
	
	public void setStudents(Student[] set_students) {
		students = set_students;
	}
	
	public CampInformation getCampInfo() {
		return campInfo;
	}
	
	public void setCampInfo(CampInformation set_campInfo) {
		campInfo = set_campInfo;
	}
	
	public Staff getStaffInCharge() {
		return staffInCharge;
	}
	
	public void setStaffInCharge(Staff set_staffInCharge) {
		staffInCharge = set_staffInCharge;
	}
	
	public CampCommitteeMember[] getCommitteeMembers() {
		return committeeMembers;
	}
	
	public void setCommitteeMembers(CampCommitteeMember[] set_committeeMembers) {
		committeeMembers = set_committeeMembers;
	}
	
} 
